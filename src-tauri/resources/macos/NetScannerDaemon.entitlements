<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Network: Allow outbound connections (for network scanning) -->
    <key>com.apple.security.network.client</key>
    <true/>

    <!-- Network: Allow inbound connections (for XPC service) -->
    <key>com.apple.security.network.server</key>
    <true/>

    <!-- Hardened Runtime: Disable JIT (not needed for daemon) -->
    <key>com.apple.security.cs.allow-jit</key>
    <false/>

    <!-- Hardened Runtime: Disable unsigned executable memory -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <false/>
</dict>
</plist>
